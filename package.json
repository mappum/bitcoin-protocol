{
  "name": "bitcoin-protocol",
  "version": "1.2.4",
  "description": "Bitcoin network protocol streams",
  "main": "index.js",
  "scripts": {
    "integration": "tap test/integration/*.js",
    "lint": "standard src test index.js",
    "test": "npm run lint && npm run unit",
    "unit": "tap --cov test/*.js",
    "build": "rm -rf lib && babel --presets es2015 src -d lib",
    "source": "rm -rf lib && ln -s src lib",
    "prepublish": "npm run build",
    "publish": "npm run source"
  },
  "keywords": [
    "bitcoin",
    "networking",
    "p2p",
    "network",
    "encode",
    "decode",
    "webcoin"
  ],
  "author": "Matt Bell <mappum@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bl": "^1.0.1",
    "create-hash": "^1.1.2",
    "from2": "^2.1.1",
    "ip": "^1.1.0",
    "through2": "^2.0.0",
    "varstruct": "^5.0.0",
    "varuint-bitcoin": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.3.13",
    "bitcoin": "^3.0.1",
    "bitcoinjs-lib": "^2.2.0",
    "coininfo": "^0.8.1",
    "standard": "^6.0.3",
    "stream-buffers": "^3.0.0",
    "tap": "^5.4.2",
    "tmp": "0.0.28"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mappum/bitcoin-protocol.git"
  },
  "bugs": {
    "url": "https://github.com/mappum/bitcoin-protocol/issues"
  },
  "homepage": "https://github.com/mappum/bitcoin-protocol#readme"
}
